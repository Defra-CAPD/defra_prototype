---
layout: layout-1-1-columns
---

<% content_for :breadcrumbs do %>
  <%= partial "partial-breadcrumbs-parcel", :locals => { :parcel => parcel} %>
<% end %>

<% content_for :column1 do %>


  <header class="page-header">
    <hgroup>
      <h1 class="js-land-use">Undefined</h1>
    </hgroup>
  </header>

  <p>
      Did the change you make happen before 2009?
  </p>


    <div class="form-group inline">
      <label for="date-old" class="block-label">
        <input type="radio" name="date" id="date-old" value="yes" class="js-toggle" data-hide="#date-past-details">
        Yes
      </label>
      <label for="date-past" class="block-label">
        <input type="radio" name="date" id="date-past" value="no" class="js-toggle" data-show="#date-past-details">
        No
      </label>
      <div class="panel-indent date js-hidden" id="date-past-details">
        <p>When did it happen?</p>
        <div class="form-group">
          <label for="dob-month" class="form-hint">Month</label>
          <input type="text" class="form-control" id="dob-month" value="">
        </div>
        <div class="form-group form-group-year">
          <label for="dob-year" class="form-hint">Year</label>
          <input type="text" class="form-control" id="dob-year" value="">
        </div>
      </div>
    </div>

<!--

  <div class="date">
    <div class="form-group">
      <label for="dob-month" class="form-hint">Month</label>
      <input type="text" class="form-control" id="dob-month" value="11">
    </div>
    <div class="form-group form-group-year">
      <label for="dob-year" class="form-hint">Year</label>
      <input type="text" class="form-control" id="dob-year" value="2014">
    </div>
  </div>
-->

  <div class="form-group">
    <a href="/<%= parcel.id %>/" class="button">Save and return</a>
    <p><a href="/<%= parcel.id %>/edit-feature">Back</a></p>
  </div>


<% end %>

<% content_for :column2 do %>

  <%= partial "maps/point" %>
  <%= partial "maps/points" %>
  <%= partial "maps/line" %>
  <%= partial "maps/shape" %>
  <%= partial "maps/hedges" %>
  <%= partial "maps/hedge" %>
  <%= partial "maps/crop" %>

<% end %>
